    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.4.1",
    "recursive-readdir": "^2.2.2",
    "source-map-support": "^0.5.21",
    "tempy": "^2.0.0",
    "tsm": "^2.2.1",
    "uvu": "^0.5.3",
    "watchlist": "^0.3.1",
    "yarn": "^1.22.17",
    "yarn-upgrade-all": "^0.5.4"
