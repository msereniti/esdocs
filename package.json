    "classnames": "^2.3.1",
    "console-feed": "^3.3.0",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.10.7",
    "downshift": "^6.1.7",
    "esbuild-mdx": "^0.2.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "json-stringify-safe": "^5.0.1",
    "picocolors": "^1.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-promise-suspense": "^0.3.3",
    "react-syntax-highlighter": "^15.4.5",
    "react-use-await": "^1.1.5",
    "source-map": "^0.7.3",
    "wouter": "^2.8.0-alpha.2",
    "yargs": "^17.3.0",
    "esbuild": "^0.14.10",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/node": "^16.11.11",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "build-esm": "^4.2.2",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-replace": "^1.0.9",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-no-cyrillic-string": "^1.0.5",
