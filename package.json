  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev.build.core": "node ./dev/core",
    "dev.run.core": "node -r source-map-support/register --experimental-specifier-resolution=node ./dist/dev/core/cli/cli.js --inputDir demo --outputDir dist/demo",
    "dev.core": "yarn dev.build.core && yarn dev.run.core",
    "dev.view": "node ./dev/view",
    "build": "node ./build/build",
    "run.build": "node ./dist/build/core/cli/cli.js --inputDir demo --outputDir dist/demo",
    "test": "tsm node_modules/uvu/bin.js src '\\.test\\.ts$'",
    "test.watch": "yarn test; watchlist src -- yarn test",
    "ts": "tsc --noEmit --project ./tsconfig.json"
  },
  "dependencies": {
    "@mdx-js/mdx": "2.0.0-rc.2",
    "@mdx-js/react": "2.0.0-rc.2",
